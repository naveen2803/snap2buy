<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark">
	<!-- host component -->
	<fx:Metadata>
		[HostComponent("spark.components.Button")]
	</fx:Metadata>
	
	<!-- states -->
	<s:states>
		<s:State name="disabled" />
		<s:State name="down" />
		<s:State name="over" />
		<s:State name="up" />
	</s:states>
	
	<!-- SkinParts
	name=iconDisplay, type=spark.primitives.BitmapImage, required=false
	name=labelDisplay, type=spark.core.IDisplayText, required=false
	-->
	<s:Rect top="0" bottom="0" left="0" right="0" alpha="0.7" radiusX="7" radiusY="7">
		<s:fill>
			<s:SolidColor color="#04212F"/>
		</s:fill>
		<s:stroke>
			<s:SolidColorStroke color="#00ECFA"/>
		</s:stroke>
	</s:Rect>
	<s:Label id="labelDisplay" paddingBottom="15" paddingLeft="15" paddingRight="15" paddingTop="15" color="#00ECFA"/>
</s:Skin>
